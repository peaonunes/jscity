{
  "name": "extractor",
  "version": "1.0.0",
  "description": "Extracts information about a JavaScript project",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "extractor",
    "javascript",
    "babel",
    "parser"
  ],
  "author": "peaonunes",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.7.3",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1"
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "jsxBracketSameLine": true
  },
  "husky": {
    "hooks": {
      "post-checkout": "if [[ $HUSKY_GIT_PARAMS =~ 1$ ]]; then yarn install --frozen-lockfile; fi",
      "post-merge": "yarn install --frozen-lockfile",
      "post-rebase": "yarn install",
      "pre-commit": "lint-staged"
    }
  }
}
